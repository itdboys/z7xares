public class WordCount{
	public static void main(String[] args){
		int count=1;  
            int[] a = {1,2,3,2,1998,1998,1956,1976,2,4,4,6,90,0,0};     
            for(int i=0;i<a.length;i+=count)  
            {  
			count = 1;
                for(int j = i+1 ; j<a.length ; j++){
				if(a[i] == a[j]){
					count += 1;
				}
			} 
            System.out.println(a[i] + " " + count); 
        	}
	}
}
